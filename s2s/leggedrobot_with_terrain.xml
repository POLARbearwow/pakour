    <!-- Ray Caster Camera Sensor: 对应Isaac Lab的RayCasterCameraCfg -->
    <plugin name="ray_caster_camera" plugin="mujoco.sensor.ray_caster_camera" 
            objtype="camera" objname="ray_caster_camera">
      <!-- 相机参数：对应Isaac Lab的PinholeCameraPatternCfg -->
      <config key="focal_length" value="11.041" />
      <config key="horizontal_aperture" value="20.955" />
      <config key="vertical_aperture" value="12.240" />
      <config key="size" value="106 60" />
      
      <!-- 测距范围：对应Isaac Lab的max_distance=2.0 -->
      <config key="dis_range" value="0.01 2.0" />
      
      <!-- 数据类型：对应Isaac Lab的data_types=["distance_to_camera"] -->
      <!-- 可以同时输出原始数据和带噪声的数据，例如：data_inf_zero data_inf_zero_noise -->
      <config key="sensor_data_types" value="data_inf_zero" />
      
      <!-- ========== Noise配置 ========== -->
      <!-- 噪声类型：uniform（均匀噪声）、gaussian（高斯噪声）、noise1（噪声+随机置零）、noise2（角度相关噪声） -->
      <!-- 取消注释下面的配置来启用噪声 -->
      
      <!-- 选项1: 均匀噪声 - 在测量值上添加 [-0.05, 0.05] 米的均匀随机噪声 -->
      <!-- <config key="noise_type" value="uniform" /> -->
      <!-- <config key="noise_cfg" value="-0.05 0.05 42" /> -->
      
